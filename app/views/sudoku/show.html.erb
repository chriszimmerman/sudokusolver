<div class="mx-auto text-center items-center">
  <h1 class="text-4xl mb-4">Solution</h1>
  <table class="outline-2" style="margin-left: auto; margin-right: auto;">
    <tr>
      <% @solution.split("").map(&:to_i).each_with_index do |number, index| %>
        <td class="w-10 border text-2xl text-center"><%= number %></td>
        <% if (index + 1) % 9 == 0 %>
          </tr>
          <tr>
        <% end %>
      <% end %>
  </table>
</div>